<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="QQMusicForAppStartTheme" parent="@android:style/Theme">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:listViewStyle">@style/DefaultPlayListView</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>
    <style name="QQMusicLockScreenTheme" parent="@android:style/Theme.Translucent.NoTitleBar">
        <item name="android:windowTranslucentStatus">true</item>
    </style>
    <style name="QQMusicTheme" parent="@android:style/Theme.Light">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:listViewStyle">@style/DefaultPlayListView</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>
    <style name="TransAppStartTheme" parent="@android:style/Theme">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:listViewStyle">@style/DefaultPlayListView</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
    </style>
</resources>

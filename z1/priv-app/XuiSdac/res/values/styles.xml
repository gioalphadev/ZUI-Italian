<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppBaseTheme" parent="@android:style/Theme.DeviceDefault.Light" />
    <style name="AppTheme" parent="@style/AppBaseTheme" />
    <style name="NewThemeWindow" parent="@android:style/Theme.DeviceDefault.Light">
        <item name="android:taskOpenEnterAnimation">@anim/anim_enter</item>
        <item name="android:taskToFrontEnterAnimation">@anim/anim_enter</item>
    </style>
    <style name="transparent" parent="@android:style/Theme.Material.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
    </style>
    <style name="myActionBarStyle" parent="@android:style/Widget.ActionBar">
        <item name="android:background">#ffd01839</item>
    </style>
    <style name="CalculatorTheme" parent="@android:style/Theme.Material.Light.NoActionBar">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:colorPrimary">&lt;color name="calculator_accent_color">#00BCD4&lt;/color></item>
        <item name="android:navigationBarColor">@android:color/black</item>
    </style>
    <style name="BrowserTheme" parent="@android:style/Theme.Holo">
        <item name="android:colorBackground">#ffffffff</item>
        <item name="android:windowBackground">" "&lt;color name="white">#ffffffff&lt;/color>" "</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowActionModeOverlay">true</item>
    </style>
</resources>
